package practice_al.prac;
public class Al10 {
	// 홀짝에 따라 다른 값 반환하기
	// 재귀와 메서드로 풀어보기
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		solution(10);
	}
	
	public static int solution(int n) {
        int num = n;
        num = num * num;
        if(num <= 1){
        	System.out.println(num);
        	return 1;
        }
        return n * solution(n-2);
    }
}
